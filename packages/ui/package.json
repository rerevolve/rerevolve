{
	"name": "@rerevolve/ui",
	"version": "0.0.0",
	"private": true,
	"exports": {
		"./components/*": "./src/components/*.tsx",
		"./hooks/*": [
			"./src/hooks/*.ts",
			"./src/hooks/*.tsx"
		],
		"./lib/*": "./src/lib/*.ts",
		"./utils/*": "./src/utils/*.ts",
		"./globals.css": "./src/styles/globals.css"
	},
	"scripts": {
		"lint": "eslint . --max-warnings 0",
		"generate:component": "turbo gen react-component",
		"check-types": "tsc --noEmit"
	},
	"devDependencies": {
		"@rerevolve/eslint-config": "workspace:*",
		"@rerevolve/typescript-config": "workspace:*",
		"@types/node": "^22.15.3",
		"@types/react": "19.1.0",
		"@types/react-dom": "19.1.1",
		"eslint": "^9.34.0",
		"typescript": "5.9.2"
	},
	"dependencies": {
		"@radix-ui/react-slot": "^1.2.3",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"lucide-react": "^0.546.0",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"tailwind-merge": "^3.3.1",
		"tw-animate-css": "^1.4.0"
	}
}

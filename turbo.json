{
	"$schema": "https://turborepo.com/schema.json",
	"ui": "tui",
	"globalEnv": ["NODE_ENV", "DATABASE_URL"],
	"tasks": {
		"@rerevolve/prisma#db-generate": {
			"cache": false
		},
		"@rerevolve/prisma#db-migrate": {
			"cache": false,
			"persistent": true
		},
		"@rerevolve/prisma#db-deploy": {
			"cache": false
		},
		"@rerevolve/prisma#db-reset": {
			"cache": false
		},
		"@rerevolve/prisma#db-studio": {
			"cache": false
		},
		"build": {
			"dependsOn": ["^build", "@rerevolve/prisma#db-generate"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": [".next/**", "!.next/cache/**"],
			"env": ["DATABASE_URL"]
		},
		"dev": {
			"dependsOn": ["@rerevolve/prisma#db-generate"],
			"cache": false,
			"persistent": true
		},
		"start": {
			"cache": false,
			"persistent": true
		},
		"lint": {
			"dependsOn": ["^lint"]
		},
		"check-types": {
			"dependsOn": ["^check-types"]
		},
		"clean": {
			"cache": false
		}
	}
}
